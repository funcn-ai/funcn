{
  "$schema": "https://funcn.ai/schemas/funcn_component.json",
  "name": "{{component_name}}",
  "version": "0.1.0",
  "description": "{{description}}",
  "type": "{{component_type}}",
  "authors": [
    {"name": "Funcn Project", "email": "info@funcn.ai"}
  ],
  "license": "MIT",
  "repository_url": "{{repository_url_if_available}}",
  "mirascope_version_min": "1.24.0",
  "files_to_copy": [
    { "source": "agent.py", "destination": "agent.py", "type": "module" },
    { "source": "tool.py", "destination": "tool.py", "type": "module" },
    { "source": "eval.py", "destination": "eval.py", "type": "module" },
    { "source": "prompt.py", "destination": "prompt.py", "type": "module" },
    { "source": "response_model.py", "destination": "response_model.py", "type": "module" },
    { "source": "mcp_server.py", "destination": "mcp_server.py", "type": "server_script" },
    { "source": "__init__.py", "destination": "__init__.py", "type": "init_file" },
    { "source": "config.json", "destination": "config.json", "type": "resource" }
  ],
  "target_directory_key": "{{target_directory_key}}",
  "python_dependencies": [],
  "registry_dependencies": [],
  "environment_variables": [],
  "example_usage": "{{example_usage_markdown_if_available}}",
  "post_add_instructions": "{{post_add_instructions}}",
  "tags": [],
  "supports_lilypad": true,
  "template_variables": {},
  "mcp_compatible": false,
  "mcp_entrypoint": ""
}
